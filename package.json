{
  "name": "react-native-nb-boilerplate",
  "version": "1.0.0",
  "description": "ReactNative template with NativeBase",
  "license": "MIT",
  "author": "tocomi",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dc": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "p:web": "expo build:web && cd web-build && vercel --prod && cd ..",
    "p:app": "expo p",
    "p:all": "yarn p:app && yarn p:web",
    "format": "prettier --check ./src",
    "format:write": "prettier --write ./src",
    "release": "dotenv release-it",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-native-community/datetimepicker": "^6.1.3",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/material-bottom-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.6",
    "date-fns": "^2.28.0",
    "expo": "^44.0.0",
    "expo-dev-client": "~0.8.0",
    "expo-localization": "~12.0.0",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "expo-updates": "~0.11.3",
    "native-base": "^3.4.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-mmkv": "1.3.2",
    "react-native-paper": "^4.12.1",
    "react-native-reanimated": "~2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-storage": "^1.0.1",
    "react-native-svg": "^12.3.0",
    "react-native-ui-lib": "^6.7.0",
    "react-native-vector-icons": "^9.1.0",
    "react-native-web": "0.17.1",
    "recoil": "^0.5.2",
    "rn-bounceable": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/config-plugins": "^4.0.15",
    "@expo/webpack-config": "~0.16.2",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/react": "~17.0.21",
    "@types/react-dom": "~17.0.9",
    "@types/react-native": "~0.64.12",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.4.1",
    "release-it": "^14.11.8",
    "typescript": "~4.3.5"
  }
}
